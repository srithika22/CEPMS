  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "node src/scripts/migration.js",
    "migrate:rollback": "node src/scripts/migration.js --rollback",
    "migrate:validate": "node src/scripts/migration.js --validate",
    "db:optimize": "node -e \"const DatabaseOptimizer = require('./src/utils/databaseOptimizer'); require('./src/config/database')().then(() => { const opt = new DatabaseOptimizer(); return opt.runCompleteOptimization(); }).then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\""
  },
